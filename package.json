{
  "name": "calorie",
  "version": "1.0.0",
  "description": "A private, mobile-friendly web application for tracking pregnancy nutrition.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint . --ext .js --fix",
    "prepare": "husky install",
    "test": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "docs": "jsdoc -c jsdoc.json",
    "security": "npm audit",
    "validate": "npm run lint && npm run test"
  },
  "keywords": [
    "pregnancy",
    "nutrition",
    "calorie-tracker",
    "food-recognition"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "winston": "^3.17.0"
  }
}
